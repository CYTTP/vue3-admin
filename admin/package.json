    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.3",
    "progress": "^2.0.3",
    "vue": "^3.2.47",
    "vue-router": "^4.2.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.21.8",
    "@types/node": "^20.2.1",
